# Database Configuration
DATABASE_URL=postgresql://dev:dev@localhost:5432/email_processing

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here

# SerpAPI Configuration (for supplier search)
SERPAPI_KEY=your-serpapi-key-here

# Email Configuration (SMTP for sending)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=test@example.com
SMTP_PASS=

# Email Configuration (IMAP for receiving)
IMAP_HOST=localhost
IMAP_PORT=1143
IMAP_USER=test@example.com
IMAP_PASS=

# Application Configuration
DEBUG=true
LOG_LEVEL=INFO

# Optional: External API Keys
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
STRIPE_API_KEY=your_stripe_api_key_here

# Optional: Override database URL (default is set in docker-compose.yml)
# DATABASE_URL=postgresql://dev:dev@localhost:5432/email_processing
